{%- capture brand -%}{%- render 'brand' -%}{%- endcapture -%}

<style>

{%- if brand == "go-to" and template.suffix contains "go-to-spf-" -%}

{%- if brand == "go-to" and template.suffix == "go-to-spf-landing" -%}
  @font-face {
    font-family: 'Recoleta';
    src: url('{{ "Recoleta-Bold.eot" | asset_url }}');
    src: url('{{ "Recoleta-Bold.eot?#iefix" | asset_url }}') format('embedded-opentype'),
        url('{{ "Recoleta-Bold.woff2" | asset_url }}') format('woff2'),
        url('{{ "Recoleta-Bold.woff" | asset_url }}') format('woff'),
        url('{{ "Recoleta-Bold.ttf" | asset_url }}') format('truetype'),
        url('{{ "Recoleta-Bold.svg#Recoleta-Bold" | asset_url }}') format('svg');
    font-weight: bold;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'Recoleta';
    src: url('{{ "Recoleta-Medium.eot" | asset_url }}');
    src: url('{{ "Recoleta-Medium.eot?#iefix" | asset_url }}') format('embedded-opentype'),
        url('{{ "Recoleta-Medium.woff2" | asset_url }}') format('woff2'),
        url('{{ "Recoleta-Medium.woff" | asset_url }}') format('woff'),
        url('{{ "Recoleta-Medium.ttf" | asset_url }}') format('truetype'),
        url('{{ "Recoleta-Medium.svg#Recoleta-Medium" | asset_url }}') format('svg');
    font-weight: 500;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'Recoleta';
    src: url('{{ "Recoleta-Regular.eot" | asset_url }}');
    src: url('{{ "Recoleta-Regular.eot?#iefix" | asset_url }}') format('embedded-opentype'),
        url('{{ "Recoleta-Regular.woff2" | asset_url }}') format('woff2'),
        url('{{ "Recoleta-Regular.woff" | asset_url }}') format('woff'),
        url('{{ "Recoleta-Regular.ttf" | asset_url }}') format('truetype'),
        url('{{ "Recoleta-Regular.svg#Recoleta-Regular" | asset_url }}') format('svg');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}


{%- endif -%}
</style>
