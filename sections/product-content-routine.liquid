          {%- if product.metafields.custom.product_routine__image_desktop_morning != blank or product.metafields.custom.product_routine__image_mobile_morning != blank -%}
            {% render "responsive-image-metafields", src_desktop: product.metafields.custom.product_routine__image_desktop_morning, src_mobile: product.metafields.custom.product_routine__image_mobile_morning %}
          {%- endif -%}
        </div>
        <div class="product-content-routine--image__evening">
          {%- if product.metafields.custom.product_routine__image_desktop_evening != blank or product.metafields.custom.product_routine__image_mobile_evening != blank -%}
            {% render "responsive-image-metafields", src_desktop: product.metafields.custom.product_routine__image_desktop_evening, src_mobile: product.metafields.custom.product_routine__image_mobile_evening %}
          {%- endif -%}
