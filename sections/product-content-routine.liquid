          <h3 class="SectionHeader__Heading Heading text--bold u-h1">
            {%- if product.metafields.custom.product_routine__title_evening != blank -%}
              {{ product.metafields.custom.product_routine__title_evening }}
            {%- else -%}
              {{ section.settings.title_evening }}
            {%- endif -%}
          </h3>
