          <h3 class="SectionHeader__Heading Heading text--bold u-h1">
            {%- if product.metafields.custom.product_routine__title_morning != blank -%}
              {{ product.metafields.custom.product_routine__title_morning }}
            {%- else -%}
              {{ section.settings.title_morning }}
            {%- endif -%}
          </h3>

          <div class="product-content-routine--text__morning-text">
            {{ product.metafields.custom.product_routine__steps-morning }}
          </div>
        </div>

